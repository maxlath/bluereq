{
  "name": "bluereq",
  "version": "2.0.1",
  "description": "A bluebird promise wrapper for HTTP requests",
  "main": "build/bluereq.js",
  "contributors": [
    "jpstevens",
    "maxlath"
  ],
  "dependencies": {
    "bluebird": "^3.0.5",
    "lodash.pick": "^4.1.0",
    "request": "^2.67.0"
  },
  "devDependencies": {
    "body-parser": "^1.3.0",
    "chai": "^1.9.1",
    "coffee-script": "^1.10.0",
    "http-status": "^0.1.7"
  },
  "scripts": {
    "test": "mocha --compilers coffee:coffee-script/register tests/unit/* tests/feature/*",
    "build": "rm -rf build; coffee -o build -c src",
    "build-watch": "rm -rf build; coffee --watch -o build -c src",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxlath/bluereq.git"
  },
  "keywords": [
